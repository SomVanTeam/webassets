warn("test 1")
task.wait(2)
function hrp()
   return game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
end
function teleport(newcf)
    if hrp() then
         hrp().CFrame = newcf
    end
end
function reportfruit(fruit)
   warn(game:HttpGet("https://somevanguy.pythonanywhere.com/report/"..fruit:gsub(" ","")))
end
warn("test 2")
local SHOPPOS = CFrame.new(63, 3, -27.5)
local SELLPOS = CFrame.new(63, 3, 1)
local FARMPOS = SHOPPOS
local PLRFARM = nil
local KEEPMOONBLOODLIT = true
local REPORTSTOCKONBUY = true
local AUTOBUYENABLED = false
local BUYPRISMATIC = true
local BUYDIVINE = true
local BUYMYTHICAL = true
local BUYELEGENDARY = true
function updatefarm()
   for _, farm in pairs(workspace.Farm:GetChildren()) do
      if farm.Sign.Core_Part.SurfaceGui.TextLabel.Text:split("'")[1] == game.Players.LocalPlayer.DisplayName then
         warn("found farm")
         PLRFARM = farm
         FARMPOS = farm.Spawn_Point.CFrame
         return
      end
   end
end
function sellall()
   warn("NE GOTOVO")
   -- pick up all fruits until full inventory/none left
   -- ignore moonlit if needed
   -- sell fruits
   -- ^ repeat until no fruits are left that can be picked up
end
warn("test 3")
local g = Instance.new("ScreenGui")
g.IgnoreGuiInset = true
local f = Instance.new("Frame")
f.Visible = false
f.Parent = g
f.Size = UDim2.new(0.25,0,0.6,0)
f.Position = UDim2.new(0.825,0,0.6,0)
f.BackgroundColor3 = Color3.fromRGB(52, 52, 52)
f.AnchorPoint = Vector2.new(0.5,0.5)
warn("test 4")
local togglef = Instance.new("TextButton")
togglef.Parent = g
togglef.BackgroundColor3 = Color3.fromRGB(127, 127, 127)
togglef.TextScaled = true
togglef.Text = "V"
togglef.Size = UDim2.new(0.03,0,0.08,0)
togglef.AnchorPoint = Vector2.new(0.5,0)
togglef.Position = UDim2.new(0.935,0,0.22,0)
togglef.Activated:Connect(function()
   f.Visible = not f.Visible
end)
warn("test 5")
local sellallbtn = Instance.new("TextButton")
sellallbtn.Parent = f
sellallbtn.BackgroundColor3 = Color3.fromRGB(91, 91, 91)
sellallbtn.TextScaled = true
sellallbtn.Text = "[UNDONE] Sell All"
sellallbtn.TextColor3 = Color3.new(1,1,1)
sellallbtn.Size = UDim2.new(1,0,0.1,0)
sellallbtn.AnchorPoint = Vector2.new(0.5,0)
sellallbtn.Position = UDim2.new(0.5,0,0,0)
sellallbtn.Activated:Connect(function()
   sellall()
end)
local autobuyseeds = sellallbtn:Clone()
autobuyseeds.Parent = f
autobuyseeds.Text = "Auto Buy Seeds"
autobuyseeds.BackgroundColor3 = Color3.fromRGB(91, 0, 0)
autobuyseeds.Position = UDim2.new(0.5,0,0.15,0)
autobuyseeds.Activated:Connect(function()
   AUTOBUYENABLED = not AUTOBUYENABLED
   if AUTOBUYENABLED then
      autobuyseeds.BackgroundColor3 = Color3.fromRGB(20, 91, 0)
   else
      autobuyseeds.BackgroundColor3 = Color3.fromRGB(91, 0, 0)
   end
end)
