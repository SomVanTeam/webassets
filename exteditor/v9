g.Parent = game.Players.LocalPlayer.PlayerGui
warn("test 7")
updatefarm()
while true do -- loop
   if AUTOBUYENABLED then
      teleport(SHOPPOS)
      for _, item in pairs(game.Players.LocalPlayer.PlayerGui.Seed_Shop.Frame.ScrollingFrame:GetChildren()) do
         local data = item:FindFirstChild("Main_Frame")
         if not data then
            continue
         end
         if data.Stock_Text.Text == "X0 Stock" then
            --continue -- skip if no stock of course
         end
         local rarity = data.Rarity_Text.Text
         --[[if rarity == "Prismatic" and not BUYPRISMATIC then
            continue
         elseif rarity == "Divine" and not BUYDIVINE then
            continue
         elseif rarity == "Mythical" and not BUYMYTHICAL then
            continue
         elseif rarity == "Legendary" and not BUYELEGENDARY then
            continue
         else
            continue
         end]]
         game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("BuySeedStock"):FireServer(item.Name)
         --if REPORTSTOCKONBUY then
            reportfruit(item.Name)
         --end
      end
      task.wait(1)
      teleport(FARMPOS)
   end
   task.wait(60)
end
